<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>
<a href="/compose">Click here to write a new blog...</a>

<% posts.forEach( post => { %>
<h1><%= post.title %></h1>
<p>
    <%= post.content.slice(0,100) + "..." %>
    <a href="/posts/<%=post._id %>">Read More</a>
</p>
<% }) %>

<%- include('partials/footer'); -%>